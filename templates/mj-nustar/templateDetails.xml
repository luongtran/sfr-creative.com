<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="3.0" type="template" client="site">
	<name>Mj-Nustar</name>
	<creationDate>19 Sep 2012</creationDate>
	<author>Mojoomla Team</author>
	<authorEmail>sales@mojoomla.com</authorEmail>
	<authorUrl>http://www.mojoomla.com</authorUrl>
	<copyright>Copyright (C) 2005 - 2012 Open Source Matters, Inc. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<version>2.5.0</version>
	<description>Mj Nustar Description</description>
	
	<files>
		<folder>images</folder>
		<folder>css</folder>
		<folder>js</folder>
		<folder>html</folder>
		<filename>index.php</filename>
		<filename>error.php</filename>
		<filename>templateDetails.xml</filename>
		<filename>template_thumbnail.png</filename>
		<filename>template_preview.png</filename>
	</files>
	
	<positions>
		<position>mj-topbar</position>
		<position>mj-left</position>
		<position>mj-right</position>
		<position>mj-contenttop</position>
		<position>mj-contentbottom</position>
		<position>mj-position1</position>
		<position>mj-position2</position>
		<position>mj-position3</position>
		<position>mj-position4</position>
		<position>mj-position5</position>
		<position>mj-position6</position>
		<position>mj-position7</position>
		<position>mj-position8</position>
		<position>mj-position9</position>
	</positions>
	
	<config>
		<fields name="params">
			<fieldset name="layout" label="Layout">
				<field name="logoFile" class="" type="media" default=""
					label="Logo"
					description="Add Logo" />
					
				<field name="googleFont"
					type="radio"
					class="btn-group"
					default="1"
					label="Google Font"
					description="Add Google Font">
					<option value="0">NO</option>
					<option value="1">YES</option>
				</field>

				<field name="googleFontName" class="" type="text" default="Electrolize"
					label="Font Name"
					description="Add Google Font Name" />
					
			</fieldset>
		</fields>
	</config>
</extension>
